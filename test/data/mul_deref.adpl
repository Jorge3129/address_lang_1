@list_add ... Nil => val, Nil => listAddr
    h = 'listAddr
    P { 'h == 0 } then | else
    @then ...
        curAddr = h;
        end
    @else ...
        L { 'h, 'Nil, P { ''j /= 0 } => j } l1
        @l1 ...
        curAddr = 'j;
        end
    @end ...
        nextNode = alloc 2;
        nextNode => curAddr
        'val => nextNode + 1
    Ret