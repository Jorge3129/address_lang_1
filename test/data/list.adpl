length = 7
firstNode = 100
allocStep = 2
firstNode => h

P { length == 0 } 0 => h |

L { 1 (1) length => i } a
    curAddr = firstNode + ('i - 1) * allocStep
    'i * 'i * 'i => curAddr + 1
    curAddr + allocStep => curAddr + 0
    P { 'i == length } 0 => curAddr + 0 |
    0 => 'curAddr
    printRefs 'curAddr
@a ...

curAddr = 0
print curAddr
printRefs 'curAddr

L { 'h, 'Nil, P { 'j /= 0 } => j } b
    print '('j + 1)
@b ...