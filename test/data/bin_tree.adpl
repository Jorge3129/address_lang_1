@create_root ... Nil => val, Nil => res_addr
    n = (alloc 2) + 0
    ptr(0) => n + 0
    'val => n + 1
    n => 'res_addr
    Ret

@create_child ... Nil => val, Nil => parent_addr, Nil => res_addr
    n = (alloc 2) + 0
    ptr('parent_addr) => n + 0
    'val => n + 1
    n => 'res_addr
    Ret

@delete ... Nil => val, Nil => node
    node_addr = 'node

    P { node_addr == 0 } Ret |

    node_val = '(node_addr + 1)

    Pg find_children { node_addr, left, right }

    P { 'val < node_val } | case_right
        Pg delete { 'val, 'left }
        end

    @case_right ...
    P { 'val > node_val } | case_cur
        Pg delete { 'val, 'right }
        end
    @case_cur ...
        parent_addr = 'node_addr
        P { 'left == 0 and 'right == 0} | case_no_left
            0 => node_addr
            end

        @case_no_left ...
        P { 'left == 0 } | case_no_right
            // P { node_val == 15 } print 4000; print '(parent_addr + 1) |
            Pg set_child { parent_addr, 'right }
            0 => node_addr
            end
        @case_no_right ...
        P { 'right == 0 } | case_both
            Pg set_child { parent_addr, 'left }
            0 => node_addr
            end
        @case_both ...

        Pg min_value { 'right, min_val }
        Pg set_value { node_addr, 'min_val }
        Pg delete { 'min_val, 'right }
    @end ...
    Ret

@set_child ... Nil => parent_addr, Nil => child_addr
    P { 'child_addr == 0 } Ret |
    'parent_addr => 'child_addr
    Ret

@set_value ... Nil => node, Nil => val
    'val => 'node + 1
    Ret

@min_value ... Nil => node, Nil => res_addr
    current = 'node

    @loop ...
        Pg find_children { current, left, right }
        P { 'left == 0 } end |
        current = 'left
    loop

    @end ... '(current + 1) => 'res_addr
    Ret

