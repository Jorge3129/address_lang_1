Pg range { 1, 1, 5, list1 }
printList list1

!

@range ... Nil => start, Nil => step, Nil => end, Nil => resAddr
    Pg listEmpty { 'resAddr }
    L { 'start ('step) 'end - 1 => i } l1
        Pg listAdd { 'i, 'resAddr }
    @l1 ...
    Ret

@listEmpty ... Nil => targetAddr
    0 => 'targetAddr
    Ret

@listAdd ... Nil => val, Nil => listAddr
    h = 'listAddr
    P { 'h == 0 } caseEmpty | caseNonEmpty
    @caseEmpty ...
        curAddr = h;
        end
    @caseNonEmpty ...
        L { 'h, 'Nil, P { ''j /= 0 } => j } l1
        @l1 ...
        curAddr = 'j;
        end
    @end ...
        nextNode = alloc 2;
        nextNode => curAddr
        'val => nextNode + 1
    Ret